{
  "name": "react-kickstarter",
  "version": "1.0.0",
  "description": "A kickstarter with a few added dependencies to build practical React apps",
  "main": "index.js",
  "repository": "https://github.com/anujit/react-kickstarter.git",
  "author": "Anujit Nene",
  "license": "ISC",
  "scripts": {
    "start": "rimraf dist && webpack-dev-server --config ./webpack.config.js --env.env=dev --open --hot",
    "build": "rimraf dist && webpack --config ./webpack.config.js --env.env=prod",
    "server": "json-server db.json",
    "dev": "npm-run-all -p server start",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "json-server": "^0.14.2",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.12.12",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
